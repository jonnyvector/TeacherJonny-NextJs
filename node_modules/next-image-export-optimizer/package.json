{
  "name": "next-image-export-optimizer",
  "version": "0.17.1",
  "description": "Optimizes all static images for Next.js static HTML export functionality",
  "main": "dist/ExportedImage.js",
  "types": "dist/ExportedImage.d.ts",
  "bin": "src/optimizeImages.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/ExportedImage.d.ts",
      "import": "./dist/ExportedImage.js"
    },
    "./future/ExportedImage": {
      "types": "./dist/future/ExportedImage.d.ts",
      "import": "./dist/future/ExportedImage.js"
    }
  },
  "typesVersions": {
    "*": {
      "future/ExportedImage": [
        "./dist/future/ExportedImage.d.ts"
      ]
    }
  },
  "files": [
    "dist/ExportedImage.js",
    "dist/ExportedImage.d.ts",
    "dist/future/ExportedImage.js",
    "dist/future/ExportedImage.d.ts",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && npx swc ./src/ExportedImage.tsx -o ./dist/ExportedImage.js && npx swc ./src/future/ExportedImage.tsx -o ./dist/future/ExportedImage.js",
    "test": "jest",
    "testFileContent": "node ./fileComparison.js",
    "test:e2e": "npx playwright install && playwright test",
    "exportExample": "cd example && npm run export && cd ..",
    "prepublishOnly": "npm run testFileContent && npm run build && npm test && npm run test:e2e",
    "fetchTags": "git fetch --tags -f"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Niels-IO/next-image-export-optimizer"
  },
  "keywords": [
    "next.js",
    "next",
    "static",
    "export",
    "image",
    "optimization",
    "webp",
    "sharp"
  ],
  "author": "Niels Grafen",
  "license": "MIT",
  "dependencies": {
    "cli-progress": "^3.10.0",
    "sharp": "^0.31.0",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "next": "12.x",
    "react": "^17.0.2 || ^18.0.0-0"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.141",
    "@types/react": "^18.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "http-server": "^14.1.0",
    "jest": "^29.0.3",
    "rimraf": "^3.0.2"
  }
}
